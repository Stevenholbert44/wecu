{
  "name": "geo-language-app",
  "version": "1.0.0",
  "description": "A web application that serves content based on user's location and language preferences.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js"
  },
  "author": "Ionos",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "isbot": "^3.6.2",
    "path": "^0.12.7",
    "ip-range-check": "^0.0.2",
    "request-ip": "^3.3.0",
    "simple-telegram-message": "^1.0.0",
    "string-similarity": "^4.0.4",
    "ua-parser-js": "^1.0.35",
    "express-rate-limit": "^7.0.0",
    "crawler-user-agents": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "express-session": "^1.17.3",
    "cookie-parser": "^1.4.6"
  }
}
{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
 